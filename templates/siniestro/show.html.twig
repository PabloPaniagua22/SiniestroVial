{% extends 'base.html.twig' %}

{% block title %}Siniestro{% endblock %}

{% block body %}
    <h1>Siniestro</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ siniestro.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ siniestro.fecha ? siniestro.fecha|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Hora</th>
                <td>{{ siniestro.hora ? siniestro.hora|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ siniestro.descripcion }}</td>
            </tr>
            <tr>
                <th>Severidad</th>
                <td>{{ siniestro.severidad }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ siniestro.estado }}</td>
            </tr>
            <tr>
                <th>Localidad</th>
                <td>{{ siniestro.localidad }}</td>
            </tr>
            <tr>
                <th>Calle</th>
                <td>{{ siniestro.calle }}</td>
            </tr>
            <tr>
                <th>Coordenadas</th>
                <td>{{ siniestro.coordenadas }}</td>
            </tr>
            <tr>
                <th>NroActa</th>
                <td>{{ siniestro.nroActa }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_siniestro_index') }}">back to list</a>

    <a href="{{ path('app_siniestro_edit', {'id': siniestro.id}) }}">edit</a>

    {{ include('siniestro/_delete_form.html.twig') }}
{% endblock %}
